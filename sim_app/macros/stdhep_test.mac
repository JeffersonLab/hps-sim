# load detector
/lcdd/url detector.lcdd

# init the run
/run/initialize

# define LHE event generator
/hps/generators/create EventGen STDHEP
/hps/generators/EventGen/file events.stdhep
/hps/generators/EventGen/verbose 4

# LCIO output
/hps/lcio/verbose 2
/hps/lcio/recreate
/hps/lcio/file stdhep_test.slcio

/run/beamOn 10
